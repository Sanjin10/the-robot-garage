{% schema %}
{
  "name": "Subscription Sections",
  "tag": "section",
  "class": "index-subscription-plan",
  "settings": [],
  "blocks": [
    {
      "type": "plans_block",
      "name": "Plans Block",
      "settings": [
        {
          "type": "select",
          "id": "accent_color",
          "label": "Color picker",
          "options": [
            {
              "label": "Green",
              "value": "green"
            },
            {
              "label": "Red",
              "value": "red"
            },
            {
              "label": "Yellow",
              "value": "yellow"
            }
          ]
        },
        {
          "type": "text",
          "id": "cardage_text",
          "label": "Age Groups"
        },
        {
          "type": "text",
          "id": "title",
          "label": "Title",
          "default": "Block Title"
        },
        {
          "type": "image_picker",
          "id": "image",
          "label": "Image"
        },
        {
          "type": "text",
          "id": "price",
          "label": "Monthly Price"
        },
        {
          "type": "text",
          "id": "price2",
          "label": "Material Fee"
        },
        {
          "type": "url",
          "id": "url",
          "label": "Learn more Link"
        },
        {
          "type": "select",
          "id": "link_color",
          "label": "Link Color",
          "options": [
            {
              "label": "Green",
              "value": "bundles-green"
            },
            {
              "label": "Red",
              "value": "bundles-red"
            },
            {
              "label": "Yellow",
              "value": "bundles-yellow"
            }
          ]
        },
        {
          "type": "select",
          "id": "button",
          "label": "Button Color",
          "options": [
            {
              "label": "Green",
              "value": "btn-green"
            },
            {
              "label": "Red",
              "value": "btn-red"
            },
            {
              "label": "Yellow",
              "value": "btn-yellow"
            }
          ]
        }
      ]
    }
  ],
  "presets": [
    {
      "name": "Subscription Plans",
      "category": "Content"
    }
  ]
}
{% endschema %}
